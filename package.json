  },
  "lint-staged": {
    "{{bin,config,lib}/**/*.js,examples/*/{src/**/*.js,react-power.config.js}}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.22.1",
    "babel-loader": "next",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.5.0",
    "css-loader": "^0.26.1",
    "deep-assign": "^2.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "node-sass": "^4.5.2",
    "opn": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^4.1.0",
    "yargs": "^7.0.2"
  },
  "author": "Chikara Chan",
  "homepage": "https://github.com/chikara-chan/react-power",
  "repository": {
    "type": "git",
    "url": "https://github.com/chikara-chan/react-power.git"
  },
  "license": "MIT",
  "devDependencies": {
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "prettier": "^1.1.0"
  }
