{
  "name": "react-power",
  "version": "0.4.0",
  "description": "Front-end development toolkit includes full-featured scripts powered by React ecosystem.",
  "keywords": [
    "cli",
    "react",
    "utility"
  ],
  "bin": {
    "react-power": "./bin/index.js"
  },
  "files": [
    "bin",
    "config",
    "lib"
  ],
  "scripts": {
    "test": "./scripts/build-test.sh",
